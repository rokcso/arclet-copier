{
  "name": "arclet-copier",
  "version": "1.6.9",
  "type": "module",
  "description": "快速复制当前页面的 URL，支持参数过滤功能",
  "scripts": {
    "dev": "node esbuild.config.js --watch",
    "build": "node esbuild.config.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run"
  },
  "keywords": [
    "chrome-extension",
    "url-copier",
    "productivity"
  ],
  "author": {
    "name": "rokcso",
    "url": "https://rokcso.com/"
  },
  "homepage": "https://github.com/rokcso/arclet-copier",
  "repository": {
    "type": "git",
    "url": "https://github.com/rokcso/arclet-copier.git"
  },
  "bugs": {
    "url": "https://github.com/rokcso/arclet-copier/issues"
  },
  "license": "GPL-3.0",
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "esbuild": "^0.25.10",
    "eslint": "^9.0.0",
    "jsdom": "^27.0.0",
    "vitest": "^3.2.4"
  }
}
